			<default>0</default>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>the level for the current pagesource</description>
		</attribute>
		<attribute>
			<name>sql</name>
			<type>string</type>
			<required>false</required>
			<description>the SQL query to execute.</description>
		</attribute>
		<attribute>
			<name>listener</name>
			<type>any</type>
			<status>hidden</status>
			<required>false</required>
			<description></description>
		</attribute>
		<attribute>
